import java.util.Scanner;
public class Solution {
	static int t,x,y,z;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		t = sc.nextInt();
		while(t-->0) {
			x += sc.nextInt();
			y += sc.nextInt();
			z += sc.nextInt();
		}
		if(x==0 && y==0 && z==0)
			System.out.println("YES");
		else 
			System.out.println("NO");
	}
}
